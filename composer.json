{
    "name": "amplexor/xconnect",
    "description": "A PHP client for the Euroscript Global Content Management (GCM) language services (http://goo.gl/VW0KK6).",
    "keywords": [
        "language",
        "service",
        "api",
        "wrapper",
        "client"
    ],
    "homepage": "https://github.com/amplexor/xconnect",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Decuyper",
            "email": "peter.decuyper@amplexor.com",
            "homepage": "http://www.amplexor.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "squizlabs/php_codesniffer": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Amplexor\\XConnect\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amplexor\\XConnect\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
